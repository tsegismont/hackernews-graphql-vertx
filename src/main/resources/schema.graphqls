type Link {
  id: ID!
  url: String!
  description: String!
}

type Query {
  allLinks: [Link]
}

type Mutation {
  createLink(url: String!, description: String!): Link
  createUser(name: String!, authProvider: AuthData!): User
}

type User {
    id: ID!
    name: String!
    email: String
    password: String
}

input AuthData {
    email: String!
    password: String!
}

schema {
  query: Query
  mutation: Mutation
}
